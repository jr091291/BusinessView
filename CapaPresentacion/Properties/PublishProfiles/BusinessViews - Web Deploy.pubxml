          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="CapaDatos.Identity.ApplicationDbContext, CapaDatos" MigrationConfiguration="CapaDatos.Migrations.Configuration, CapaDatos" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)BusinessViewConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:businessviews.database.windows.net,1433;Initial Catalog=businessviews;Persist Security Info=False;User ID=jr;Password=Business2016;MultipleActiveResultSets=False;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>